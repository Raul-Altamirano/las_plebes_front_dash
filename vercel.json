{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
    "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "env": {

  },
    "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
